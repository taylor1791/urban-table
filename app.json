{
  "expo": {
    "name": "Urban Table",
    "description": "Practice math tables at the safety of your fingers",
    "slug": "urban-table",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
